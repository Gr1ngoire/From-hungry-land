{
  "name": "from-hungry-land",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:build": "tsc -watch",
    "start:run": "nodemon build/server/src/index.js",
    "start": "concurrently npm:start:*",
    "typeorm": "typeorm-ts-node-commonjs -d ./src/app-data-source.ts",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:generate": "npm run typeorm -- migration:generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "concurrently": "7.6.0",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "jsonwebtoken": "9.0.0",
    "nodemon": "2.0.20",
    "pg": "8.9.0",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.3.11"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/express": "4.17.16",
    "@types/jsonwebtoken": "9.0.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register build/server/src/index.js",
    "ext": "ts, js"
  }
}
